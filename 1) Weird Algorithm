#include<bits/stdc++.h>
using namespace std;
 
void c_p_c()
{
	#ifndef ONLINE_JUDGE
	freopen("input.txt","r",stdin);
	freopen("output.txt","w",stdout);
	#endif   
}
 
int main(){
	c_p_c();	
 
	long long int n;
	cin>>n;
 
	while(n!=1){
		cout<<n<<" ";
		if(n%2 != 0){
			n = n*3 + 1;
		}
		else{
			n/=2;
		}
	}
	cout<<n<<endl;
}
